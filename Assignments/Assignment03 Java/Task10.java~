import java.util.Scanner;
public class Task10
{
    public static void main(String[] args)
    {
        Scanner sc = new Scanner ( System.in );
        char name;
        System.out.println("Please Car Name: ");
        name = sc.next().charAt(0);
        System.out.println(name);
        System.out.println(name);
        System.out.println(name);
        System.out.println(name);
    }
}