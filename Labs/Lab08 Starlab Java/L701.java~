import java.util.Scanner;
public class L701
{
    public static void main(String[] args)
    { 
        Scanner sc= new Scanner(System.in);
        System.out.println("Please enter number");
        int num = sc.nextInt();
        int c=1;
        while(c<=num)
        {
            System.out.print(c);
            ++c;
        }
        System.out.println(); 
    }      
}
